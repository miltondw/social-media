{
    "version": 1.5,
    "name": "social-media-merng",
    "builds": [
        {
            "use": "@vercel/static-build",
            "src": "package.json"
        }
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}